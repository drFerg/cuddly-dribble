journal = {International Conference on Embedded Wireless Systems and Networks (EWSN)},
title = {{Energy Neutral Operation of Vibration Energy-Harvesting Sensor Networks for Bridge Applications}},
url = {https://ecc153fc-a-3b850097-s-sites.googlegroups.com/a/ewsn.org/ewsn/file-repository/ewsn-2018/001{\_}012{\_}gaglione.pdf?attachauth=ANoY7coy34cd6ViNLsRPJe2QuBZSuzSe8eldlHx3W7BPO7vRLTV6WQjOV3NG-cv7DVJ4FHw3XZ7OE8VlGjVyfNecK5HQaFQxv43uzXbg9Q4vj{\_}FEM9GlEqz8belOCiAt},
