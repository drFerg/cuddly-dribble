title = {{A Competition to Push the Dependability of Low-Power Wireless Protocols to the Edge}},
url = {https://ecc153fc-a-3b850097-s-sites.googlegroups.com/a/ewsn.org/ewsn/file-repository/ewsn2017/054{\_}065{\_}schuss.pdf?CFID=12707478{\&}CFTOKEN=22ce90b2bab540ee-A5BFFA2C-923A-D596-52A10334B5025857{\&}attachauth=ANoY7cq96BMnlecWRTephkUi{\_}rZbUp1G{\_}DnJurUPsViLb-0x2mrB39Rc},
